<?xml version="1.0" encoding="utf-8"?>
<project>

	<meta title="BunnyMark" package="com.drkibitz.bunnymark" version="1.0.0" />
	<app path="Export" file="BunnyMark" main="bunnymark.pixi.Main" />

	<window background="#FFFFFF" fps="60" />
	<window hardware="true" require-shaders="true" depth-buffer="false" stencil-buffer="false" />
	<window fullscreen="false" resizable="true" borderless="false" vsync="false" />
	<window orientation="portrait" />
	<window width="0" height="0" if="html5" />

	<source path="Source" />

	<haxelib name="openfl" />

	<ios binaries="armv7" />

	<assets path="Assets/images" rename="images" include="*" />

	<icon path="Assets/images/bunny.png" />
</project>
