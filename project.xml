<?xml version="1.0" encoding="utf-8"?>
<project>

	<meta title="BunnyMark" package="com.drkibitz.bunnymark" version="1.0.0" />
	<app path="Export" file="BunnyMark" main="bunnymark.BlitBunnyMark" />

	<window fps="60" />
	<window width="800" height="600" if="desktop" />
